#fibonacci series
print("Fibonacci Series")
n1=input("Enter the number: ")
n=int(n1)
if(n>0):
  f=0
  s=1
  i=0
  next=0
  while(1):
    print("\n\n*******************************************")
    for i in range(n+1):
      if(i<=1):
        print(i)
      else:
        next=f+s
        f=s
        s=next
        print(next)
    i=i+1
    break
else:
  print("you have entered wrong input",n)
  exit()

